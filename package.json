{
  "name": "pearl-frontend",
  "version": "1.6.2",
  "description": "Web application of Pearl Land Use and Land Classification (LULC) project.",
  "repository": {
    "type": "git",
    "url": "https://github.com/developmentseed/pearl-frontend.git"
  },
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/developmentseed/pearl-frontend/issues"
  },
  "homepage": "https://github.com/developmentseed/pearl-frontend",
  "scripts": {
    "postinstall": "[ -f app/assets/scripts/config/local.js ] || echo \"'use strict';\n\nexport default {\n  environment: 'development',\n};\" > app/assets/scripts/config/local.js",
    "serve": "gulp serve",
    "serve:production": "NODE_ENV=production gulp serve",
    "serve:cypress": "NODE_ENV=cypress gulp serve",
    "serve:testing": "NODE_ENV=testing gulp serve",
    "build": "NODE_ENV=production gulp",
    "build:reforestamos": "NODE_ENV=reforestamos gulp",
    "build:reforestamos-prod": "NODE_ENV=reforestamos-prod gulp",
    "stage": "NODE_ENV=staging gulp",
    "lint": "eslint app/assets/scripts/ cypress/ --ext .js",
    "lint:css": "stylelint 'app/assets/styles/**/**' 'app/assets/scripts/**/*.js'",
    "cy:open": "NODE_ENV=cypress cypress open --e2e --browser electron",
    "cy:open:stress": "NODE_ENV=testing cypress open --config-file cypress-stress.json",
    "cy:run": "NODE_ENV=cypress cypress run --e2e --browser electron"
  },
  "engines": {
    "node": "14.x"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ],
          "plugins": [
            "babel-plugin-styled-components",
            "@babel/plugin-transform-spread",
            "@babel/plugin-proposal-object-rest-spread"
          ]
        }
      ],
      "require-globify",
      "envify"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
